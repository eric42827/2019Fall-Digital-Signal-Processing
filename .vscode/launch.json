    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/dsp_hw1/train",
            "args": ["100", "model_init.txt", "data/train_seq_01.txt", "model_01.txt"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/dsp_hw1",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
    ]
}